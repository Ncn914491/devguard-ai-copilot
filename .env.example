# DevGuard AI Copilot Environment Configuration
# Copy this file to .env and fill in your actual values

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# GitHub OAuth Configuration (configured in Supabase Auth)
GITHUB_CLIENT_ID=your_github_oauth_client_id_here
GITHUB_CLIENT_SECRET=your_github_oauth_client_secret_here
GITHUB_OAUTH_REDIRECT_URI=http://localhost:8080/auth/github/callback

# AI Backend Configuration
GEMINI_API_KEY=your_gemini_api_key_here

# Email Configuration (can be mocked for demo)
EMAIL_SMTP_HOST=smtp.gmail.com
EMAIL_SMTP_PORT=587
EMAIL_USERNAME=your_email@gmail.com
EMAIL_PASSWORD=your_app_password

# Feature Toggles
ENABLE_EMAIL_VERIFICATION=false
ENABLE_OAUTH=true
ENABLE_DEMO_MODE=true

# Development Settings
DEBUG_MODE=true
LOG_LEVEL=INFO